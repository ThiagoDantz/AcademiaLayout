<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title>Formulário de Entrada de Número</title>
    <script type="text/javascript">
        function validarNumeros(e) {
            var tecla = e.key;
            var codigoTecla = e.which || e.keyCode;
            if (codigoTecla < 48 || codigoTecla > 57) { // Verifica se a tecla pressionada não é um número
                e.preventDefault();
            }
        }
    </script>
</head>
<body>
    <form id="form1" runat="server">
        <div>
            <asp:TextBox ID="txtNumero" runat="server" onkeypress="validarNumeros(event)" />
            <asp:Button ID="btnSalvar" runat="server" Text="Salvar" OnClick="btnSalvar_Click" />
        </div>
    </form>
</body>
</html>


---------------------------------------------------------------------------------------------------

using System;
using System.Data;
using System.Data.SqlClient;
using System.Configuration;

namespace SeuNamespace
{
    public partial class Default : System.Web.UI.Page
    {
        protected void btnSalvar_Click(object sender, EventArgs e)
        {
            // Recupera o valor do TextBox
            string numero = txtNumero.Text;

            // Chama a procedure no SQL Server
            SalvarNoBanco(numero);
        }

        private void SalvarNoBanco(string numero)
        {
            string connectionString = ConfigurationManager.ConnectionStrings["SuaConexao"].ConnectionString;

            using (SqlConnection con = new SqlConnection(connectionString))
            {
                using (SqlCommand cmd = new SqlCommand("SuaProcedure", con))
                {
                    cmd.CommandType = CommandType.StoredProcedure;

                    // Adiciona o parâmetro da procedure
                    cmd.Parameters.Add(new SqlParameter("@Numero", numero));

                    // Abre a conexão e executa a procedure
                    con.Open();
                    cmd.ExecuteNonQuery();
                }
            }
        }
    }
}

----------------------------------------------------------------------------
<asp:ListView ID="ListView1" runat="server">
    <ItemTemplate>
        <asp:TextBox ID="TextBox1" runat="server" />
        <asp:Button ID="Button1" runat="server" Text="Salvar" CommandName="Save" CommandArgument='<%# Container.DisplayIndex %>' />
    </ItemTemplate>
</asp:ListView>

______________________________________
protected void ListView1_ItemCommand(object sender, ListViewCommandEventArgs e)
{
    if (e.CommandName == "Save")
    {
        // Encontre o índice do item
        int index = Convert.ToInt32(e.CommandArgument);

        // Encontre o item correspondente
        ListViewItem item = ListView1.Items[index];

        // Encontre a TextBox dentro desse item
        TextBox txt = (TextBox)item.FindControl("TextBox1");

        if (txt != null)
        {
            // Capture o texto digitado
            string textoDigitado = txt.Text;

            // Agora você pode usar o valor capturado
            // Exemplo: salvar no banco de dados, mostrar na tela, etc.
            Response.Write("Valor capturado: " + textoDigitado);
        }
    }
}
